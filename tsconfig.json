{
  "compilerOptions": {
    "target": "es2017",
    "module": "node16",
     "moduleResolution": "node10",
     "baseUrl": "./engine/src",
     "paths": {
        "@/*": ["./*"],
        "@modules/*": ["./modules/*"],
        "@utils/*": ["./utils/*"],
        "@libs/*": ["./libs/*"]
     },
     "types": ["@types/jest", "node"],
     "allowJs": true,
     "sourceMap": true,
     "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
     "noImplicitAny": true,
    "skipLibCheck": true
  },
  "exclude": [
    "**/__tests__/**",
    "**/__mocks__/**"
  ]
}
